    "chalk": "^1.1.3",
    "chunk-manifest-webpack-plugin": "^0.1.0",
    "compression": "^1.6.2",
    "cookie-parser": "1.4.3",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "express-state": "^1.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "helmet": "^3.3.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
